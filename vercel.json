{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "MY_BACKEND_URL": "https://my-dummy-ticket-backend.vercel.app"
  },
  "cors": {
    "origin": "https://my-dummy-ticket.vercel.app",
    "methods": ["GET", "POST", "PUT", "DELETE"]
  }
}
